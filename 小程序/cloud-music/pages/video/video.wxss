page{
  width: 100%;
  /* height: 1500rpx; */
  height: 100%;
}
.header{
  /* position: relative; */
  position: sticky;
  top:0;
  z-index: 100;
  width: 100%;
  height: 96rpx;
  display: flex;
  background-color: #008c8c;  
}
.container,.swiper,.swiper-item{
  /* height: 4000rpx;   */
  /* height: 100%; */
  width: 100%;
  height: 900%;
}
.title{
  flex: 1;
  text-align: center;
  line-height: 96rpx;
  color: #ffffff;
}
.title1{
  width: 40rpx;
  height: 10rpx;
  position: absolute;
  left: 55rpx;bottom: 13rpx;
  background-color: #fff;
  border-radius: 10rpx;
  transition: transform 0.6s;
}
.item{
  width: 100%;
  height: 350rpx;
  margin-bottom: 10rpx;
}
.item-container{
  width: 100%;
  height: 100%;
  position: relative;
  background-size: cover;
}
.item-title{
  position: absolute;
  color: red;
  font-size: 60rpx;
  bottom: 65rpx;
  left: 30rpx;
}
.item-title1{
  position: absolute;
  color: #fff;
  bottom: 25rpx;
  left: 30rpx;
  font-size: 35rpx;
  white-space: normal;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.item-title2{
  position: absolute;
  color: #fff;
  right: 10rpx;
  top: 0;
}
